{
  "compilerOptions": {
    "target": "ES2016",
    "preserveValueImports": false,
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["ESNEXT", "DOM"],
    "esModuleInterop": true,
    "strict": true,
    "outDir": "./build",
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ],
    "strictNullChecks": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"],
  "plugins": [
    {
      "transform": "typia/lib/transform"
    }
  ],

  "strictNullChecks": true
}
